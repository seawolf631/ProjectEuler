let integer = 13195,
    primeArray = [],
    isPrime;

for(let i = 2; i <= integer; i++){
    if(integer % i === 0){
	for(let j = 2; j <= j/2; j++){
	    if(i % j ===0)
		isPrime = false;
	    else
		isPrime = true;
	}
    

    if(isPrime === true){
	integer /= i;
	primeArray.push(i);
    }
    }
}